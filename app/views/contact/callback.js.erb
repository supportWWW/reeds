<% if @success -%>
	$('#callback_form_notice').html(<%= flash[:public_notice].to_json %>);
	$('#callback_form_notice').show();
	$('#google_conversion').html(<%= js_partial "shared/google_conversion" %>);
	<% flash[:public_notice] = nil %>
<% else -%>
	$('#callback_form_errors').html(<%= error_messages_for(:form).to_json %>);
	$('#callback_form_errors').show();
<% end -%>