<% content_for(:browser_title) do %>
	Branches
<% end -%>

<% content_for(:title) do %>
	<h2>Branches</h2>
<% end -%>



<% unless @branches.blank? %>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for branch in @branches %>
  <tr>
    <td><%= link_to  branch.name, branch %></td>
  </tr>
<% end %>
</table>

<% else  %>
  <p>There are no branches available.</p>
<% end  %>
