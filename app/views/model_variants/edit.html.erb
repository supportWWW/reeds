<h1>Editing model_variant</h1>

<% form_for(@model_variant) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :model_id %><br />
    <%= f.text_field :model_id %>
  </p>
  <p>
    <%= f.label :year %><br />
    <%= f.text_field :year %>
  </p>
  <p>
    <%= f.label :mead_mcgrouther_code %><br />
    <%= f.text_field :mead_mcgrouther_code %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @model_variant %> |
<%= link_to 'Back', model_variants_path %>
