<h2>Editing menu item</h2>

<% form_for :menu_item, :url => admin_menu_item_path(@menu_item), :html => { :method => :put }  do |f| -%>
	<%= render :partial =>  "form", :locals => { :f => f } %>

	<p>
	  <%= f.submit "Update" %>
	</p>
<% end %>

<%= link_to 'Show', admin_menu_item_path(@menu_item) %> |
<%= link_to 'Back', admin_menu_items_path %>
